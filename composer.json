{
    "name": "php-resta/skeleton",
    "type": "library",
    "description": "resta php api restfull framework",
    "keywords": ["php","restapi","restfull"],
    "homepage": "https://github.com/aligurbuz/resta",
    "license": "MIT",
    "authors": [
        {
            "name": "aligurbuz",
            "email": "galiant781@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/app",
            "Src\\": "src",
            "Boot\\": "src/boot",
            "Store\\": "src/store",
            "Providers\\": "src/providers",
            "Bootstrapper\\" : "bootstrapper",
            "Test\\" : "tests"

        },
        "files": []
    },
    "require": {
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ocramius/package-versions": "^1.5",
        "php-resta/resta-core": "dev-master",
        "zendframework/zend-config": "^3.2",
        "zendframework/zend-crypt": "^3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload -o",
            "php api key generate"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    },
    "require-dev": {
        "nunomaduro/collision": "^3.0"
    }
}
