{
    "name": "aligurbuz/resta",
    "type": "library",
    "description": "resta php api restfull framework",
    "keywords": ["php","restapi","restfull"],
    "homepage": "https://github.com/aligurbuz/resta",
    "license": "MIT",
    "authors": [
        {
            "name": "aligurbuz",
            "email": "galiant781@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/app",
            "Src\\": "src",
            "Boot\\": "src/boot",
            "Store\\": "src/store",
            "Providers\\": "src/providers",
            "Bootstrapper\\" : "bootstrapper",
            "Test\\" : "tests"
        },
        "files": []
    },
    "require": {
        "restapix/resta":"dev-master",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "zendframework/zend-crypt": "^3.3"
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload -o",
            "php api key generate"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    }
}
